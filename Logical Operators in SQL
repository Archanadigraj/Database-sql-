1.AND
SELECT EmpName, Salary
FROM Employees
WHERE Salary > 50000 AND DeptID = 102;
----------------------------------------------
2.OR
SELECT EmpName, Salary
FROM Employees
WHERE Salary = 50000 OR DeptID IS NULL;
--------------------------------------
3.NOT
SELECT EmpName, Salary
FROM Employees
WHERE NOT Salary = 50000;
------------------------------
4.BETWEEN
SELECT EmpName, Salary
FROM Employees
WHERE Salary BETWEEN 55000 AND 70000;
--------------------------------------
5.IN
SELECT EmpName, DeptID
FROM Employees
WHERE DeptID IN (101, 103);
------------------------------------------
6.NOT IN
SELECT EmpName, DeptID
FROM Employees
WHERE DeptID NOT IN (101, 103);
--------------------------------------
7.LIKE
SELECT EmpName
FROM Employees
WHERE EmpName LIKE 'A%';   -- starts with A
--------------------------------------------------
SELECT EmpName
FROM Employees
WHERE EmpName LIKE '_o%';   -- 2nd letter is 'o'
--------------------------------------------
SELECT EmpName
FROM Employees
WHERE EmpName LIKE '%a';   -- ends with 'a'
------------------------------------------
